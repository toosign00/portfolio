{
  "rewrites": [
    {
      "source": "/projects/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "toosign.kr"
        }
      ],
      "destination": "https://toosign.kr",
      "permanent": true
    }
  ]
}
