{
  "name": "hyunsoo-portfolio",
  "private": true,
  "version": "2.1.4",
  "type": "module",
  "description": "프론트엔드 개발자 노현수의 포트폴리오 웹사이트",
  "author": "Hyunsoo Ro",
  "homepage": "https://toosign.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/toosign00/portfolio"
  },
  "keywords": [
    "portfolio",
    "react",
    "typescript",
    "frontend",
    "developer"
  ],
  "scripts": {
    "dev": "vite --host",
    "prebuild": "node scripts/generate-sitemap.js",
    "build": "tsc && vite build",
    "analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "clean": "rm -rf dist node_modules/.vite",
    "compile": "pnpm clean && pnpm build && pnpm preview",
    "lint": "biome lint --write",
    "type-check": "tsc --noEmit --project tsconfig.app.json",
    "format": "biome format --write",
    "tw:sort": "biome lint --write --unsafe",
    "check": "pnpm type-check && pnpm lint && pnpm format",
    "reporter": "biome check --reporter=summary"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.4",
    "@supabase/supabase-js": "^2.89.0",
    "@tanstack/react-query": "^5.90.12",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "es-toolkit": "^1.43.0",
    "i18next": "^25.7.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "motion": "^12.23.26",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-ga4": "^2.1.0",
    "react-hook-form": "^7.68.0",
    "react-i18next": "^16.5.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.11.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@tailwindcss/vite": "^4.1.18",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "vite": "^7.3.0"
  }
}
